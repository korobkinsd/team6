<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.staff.api.entities.RoleEntity" table="role" schema="team6">
        <id name="id" column="id"/>
        <property name="name" column="name"/>
        <property name="isAdmin" column="isAdmin"/>
        <set name="userrolesById" inverse="true">
            <key>
                <column name="idRole"/>
            </key>
            <one-to-many not-found="ignore" class="com.staff.api.entities.UserrolesEntity"/>
        </set>
    </class>
</hibernate-mapping>