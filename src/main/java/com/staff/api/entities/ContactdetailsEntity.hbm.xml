<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.staff.api.entities.ContactdetailsEntity" table="contactdetails" schema="team6">
        <property name="idCandidate" column="idCandidate"/>
        <property name="idContactType" column="idContactType"/>
        <property name="contactDetails" column="contactDetails"/>
        <many-to-one name="candidateByIdCandidate" class="com.staff.api.entities.CandidateEntity">
            <column name="idCandidate"/>
        </many-to-one>
        <many-to-one name="contacttypeByIdContactType" class="com.staff.api.entities.ContacttypeEntity">
            <column name="idContactType"/>
        </many-to-one>
    </class>
</hibernate-mapping>