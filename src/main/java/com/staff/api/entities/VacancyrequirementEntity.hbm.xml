<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.staff.api.entities.VacancyrequirementEntity" table="vacancyrequirement" schema="team6">
        <property name="idVacancy" column="idVacancy"/>
        <property name="idSkill" column="idSkill"/>
        <many-to-one name="vacancyByIdVacancy" class="com.staff.api.entities.VacancyEntity">
            <column name="idVacancy"/>
        </many-to-one>
        <many-to-one name="skillByIdSkill" class="com.staff.api.entities.SkillEntity">
            <column name="idSkill"/>
        </many-to-one>
    </class>
</hibernate-mapping>