<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.staff.api.entities.UserrolesEntity" table="userroles" schema="team6">
        <property name="idUser" column="idUser"/>
        <property name="idRole" column="idRole"/>
        <many-to-one name="userByIdUser" class="com.staff.api.entities.UserEntity">
            <column name="idUser"/>
        </many-to-one>
        <many-to-one name="roleByIdRole" class="com.staff.api.entities.RoleEntity">
            <column name="idRole"/>
        </many-to-one>
    </class>
</hibernate-mapping>