<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.staff.api.entities.CandidatecompetenceEntity" table="candidatecompetence" schema="team6">
        <property name="idCandidate" column="idCandidate"/>
        <property name="idSkill" column="idSkill"/>
        <many-to-one name="candidateByIdCandidate" class="com.staff.api.entities.CandidateEntity">
            <column name="idCandidate"/>
        </many-to-one>
        <many-to-one name="skillByIdSkill" class="com.staff.api.entities.SkillEntity">
            <column name="idSkill"/>
        </many-to-one>
    </class>
</hibernate-mapping>