<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.staff.api.entities.CandidateEntity" table="candidate" schema="team6">
        <id name="id" column="id"/>
        <property name="email" column="email"/>
        <property name="name" column="name"/>
        <property name="surname" column="surname"/>
        <property name="birthday" column="birthday"/>
        <property name="photoFilePath" column="photoFilePath"/>
        <property name="salary" column="salary"/>
        <property name="idCandidateState" column="idCandidateState"/>
        <many-to-one name="candidatestateByIdCandidateState" class="com.staff.api.entities.CandidatestateEntity">
            <column name="idCandidateState" not-null="true"/>
        </many-to-one>
        <set name="attachmentsById" inverse="true">
            <key>
                <column name="idCandidate"/>
            </key>
            <one-to-many not-found="ignore" class="com.staff.api.entities.AttachmentEntity"/>
        </set>
        <set name="candidatecompetencesById" inverse="true">
            <key>
                <column name="idCandidate"/>
            </key>
            <one-to-many not-found="ignore" class="com.staff.api.entities.CandidatecompetenceEntity"/>
        </set>
        <set name="candidateexperiencesById" inverse="true">
            <key>
                <column name="idCandidate"/>
            </key>
            <one-to-many not-found="ignore" class="com.staff.api.entities.CandidateexperienceEntity"/>
        </set>
        <set name="contactdetailsById" inverse="true">
            <key>
                <column name="idCandidate"/>
            </key>
            <one-to-many not-found="ignore" class="com.staff.api.entities.ContactdetailsEntity"/>
        </set>
        <set name="interviewsById" inverse="true">
            <key>
                <column name="idCandidate"/>
            </key>
            <one-to-many not-found="ignore" class="com.staff.api.entities.InterviewEntity"/>
        </set>
        <set name="vacancycandidatesById" inverse="true">
            <key>
                <column name="idCandidate"/>
            </key>
            <one-to-many not-found="ignore" class="com.staff.api.entities.VacancycandidatesEntity"/>
        </set>
    </class>
</hibernate-mapping>