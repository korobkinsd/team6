<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.staff.api.entities.VacancyEntity" table="vacancy" schema="team6">
        <id name="id" column="id"/>
        <property name="position" column="position"/>
        <property name="idDeveloper" column="idDeveloper"/>
        <property name="salaryFrom" column="salaryFrom"/>
        <property name="salaryTo" column="salaryTo"/>
        <property name="isActive" column="isActive"/>
        <many-to-one name="userByIdDeveloper" class="com.staff.api.entities.UserEntity">
            <column name="idDeveloper"/>
        </many-to-one>
        <set name="interviewsById" inverse="true">
            <key>
                <column name="idVacancy"/>
            </key>
            <one-to-many not-found="ignore" class="com.staff.api.entities.InterviewEntity"/>
        </set>
        <set name="vacancycandidatesById" inverse="true">
            <key>
                <column name="idVacancy"/>
            </key>
            <one-to-many not-found="ignore" class="com.staff.api.entities.VacancycandidatesEntity"/>
        </set>
        <set name="vacancyrequirementsById" inverse="true">
            <key>
                <column name="idVacancy"/>
            </key>
            <one-to-many not-found="ignore" class="com.staff.api.entities.VacancyrequirementEntity"/>
        </set>
    </class>
</hibernate-mapping>