<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.staff.api.entities.CandidateexperienceEntity" table="candidateexperience" schema="team6">
        <property name="idCandidate" column="idCandidate"/>
        <property name="dateFrom" column="dateFrom"/>
        <property name="dateTo" column="dateTo"/>
        <property name="jobDescription" column="jobDescription"/>
        <property name="jobPosition" column="jobPosition"/>
        <property name="companyName" column="companyName"/>
        <many-to-one name="candidateByIdCandidate" class="com.staff.api.entities.CandidateEntity">
            <column name="idCandidate"/>
        </many-to-one>
    </class>
</hibernate-mapping>