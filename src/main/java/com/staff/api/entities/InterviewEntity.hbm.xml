<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.staff.api.entities.InterviewEntity" table="interview" schema="team6">
        <id name="id" column="id"/>
        <property name="idVacancy" column="idVacancy"/>
        <property name="planDate" column="planDate"/>
        <property name="idCandidate" column="idCandidate"/>
        <property name="factDate" column="factDate"/>
        <many-to-one name="vacancyByIdVacancy" class="com.staff.api.entities.VacancyEntity">
            <column name="idVacancy"/>
        </many-to-one>
        <many-to-one name="candidateByIdCandidate" class="com.staff.api.entities.CandidateEntity">
            <column name="idCandidate"/>
        </many-to-one>
        <set name="interviewfeedbacksById" inverse="true">
            <key>
                <column name="idInterview"/>
            </key>
            <one-to-many not-found="ignore" class="com.staff.api.entities.InterviewfeedbackEntity"/>
        </set>
    </class>
</hibernate-mapping>